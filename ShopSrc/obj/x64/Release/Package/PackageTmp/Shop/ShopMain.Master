            <div id="SliderBox" runat="server" class="slidertop col-md-9 col-sm-12">
                 <div id="Slidertop" class="carousel slide" data-ride="carousel"   >

                <!-- Indicators -->
                <ul class="carousel-indicators">
                    <% NM.Shop.ShopMain nss = new NM.Shop.ShopMain();
                        System.Data.DataView SDDV = nss.getslider();
                        string[] imags = SDDV[0]["SlidePics"].ToString().Split("©".ToCharArray());
                        
                        int k = 0;
                        foreach (string imgg in imags)
                        {
                            if (imgg != "")
                            {
                                  %>
                    <% if (k == 0) { %>

                    <li data-target="#Slidertop" data-slide-to="0" class="active"></li>
                     <%  k = k + 1;
                         }else
                                 {
                                     %>

                    <li data-target="#Slidertop" data-slide-to="<% Response.Write(k); %>"></li>
                     <% k = k + 1;
                                 }
                             }
                         } %>
                   
                </ul>

                <!-- The slideshow -->
                <div class="carousel-inner" style="border-radius:10px">
                     <%

                         imags = SDDV[0]["SlidePics"].ToString().Split("©".ToCharArray());
                         string[] linkslide = SDDV[0]["linkSlide"].ToString().Split(",".ToCharArray());
                         int m = 0;
                         for (int i= 0; i <imags.Length ;i++ )
                         {
                             if (imags[i] != "")
                             {
                                 m = m + 1;  %>
                    <% if (m == 1)
                        { %>
                    <div class="carousel-item active">
                        <% if (linkslide.Length == imags.Length)
                            { %>
                        <a href="<% if (linkslide[i] != "") { Response.Write(linkslide[i]); } else { Response.Write("#"); } %>">
                            <%} %>
                        <img src="<%= imags[i] %>" alt="Los Angeles" width="1100" height="500">
                            </a>
                    </div>
                    <% }
                        else
                        {
                    %>
                    <div class="carousel-item ">
                        <%object @__o; if (linkslide.Length == imags.Length)
                            { %>
                        <a href="<% if (linkslide[i] != "") { Response.Write(linkslide[i]); } else { Response.Write("#"); } %>">
                            <%} %>
                        <img src="<%= imags[i] %>" alt="Los Angeles" width="1100" height="500">
                            </a>
                    </div>

                    <%
                                }
                            }
                        } %>
                    
                </div>

