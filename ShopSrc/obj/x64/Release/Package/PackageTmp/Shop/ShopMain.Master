    <script type="text/javascript">
        function rr() {
            NM.Shop.Admin.ShopAdmin.getproducts(WASucsess, WAError);
            return false;
            //alert(Ajax_Wecan(data, url));
        };
        function WASucsess(r) {
            fin("prod").innerHTML = r;
        };
        function addtocart(id) {
            NM.Shop.Admin.ShopAdmin.Set_Get_BI("Set", "bascket", id, Set_Get_BI_SC0, WAError);
        };
        function Set_Get_BI_SC0(r) {
            NM.Shop.Admin.ShopAdmin.Set_Get_BI("get", "bascket", 0, Set_Get_BI_SC01, WAError);
            if (r != null)
                alert(r);
        };
        function Set_Get_BI_SC00(r) {

            NM.Shop.Admin.ShopAdmin.Set_Get_BI("get", "bascket", 0, Set_Get_BI_SC01, WAError);
        };
        function deletfromcart(id) {
            NM.Shop.Admin.ShopAdmin.Set_Get_BI("delete", "bascket", id, Set_Get_BI_SC00, WAError);
        }
       
        function Set_Get_BI_SC01(r) {
            var rty = r;
            var sumtp = 0;
            var htll = "";
            fin("sabadcount").innerHTML = rty.length;
            for (var i = 0; i < rty.length; i++) {
                htll += '<!-- Cart Box Start --> ' +
                    ' <div class="single-cart-box"> ' +
                    '<div class="cart-img"> ' +
                    '<a href="#"><img src="' + javalist(rty[i], "imageurl") + '" alt="cart-image"></a> ' +
                    '</div>' +
                    '<div class="cart-content"> ' +
                    '<h6><a href="product.aspx?PID=' + javalist(rty[i], "PId") + '">' + javalist(rty[i], "PName") + '</a></h6>' +
                    '<span class="cart-price">' + javalist(rty[i], "NPrice") + ' تومان</span>' +
                    '<span class="pro-quantity">تعداد:' + javalist(rty[i], "CNT") + '</span> ' +
                    '<span class="pro-quantity" style="white-space: pre;">' + javalist(rty[i], "Property") +'</span>'+
                    '</div>' +
                    '<a class="del-icone" href="javascript:deletfromcart(' + javalist(rty[i], "PId") + ') "><i class="ion-close"></i></a>' +
                    '</div>' +
                    '<!-- Cart Box End -->';
                sumtp += parseInt(javalist(rty[i], "NPrice"));
            }
            var mali = 0;
            mali = parseInt(sumtp * 9 / 100);
            sumtp = parseInt(sumtp + (sumtp * 9 / 100));
            var footer = '<!-- Cart Footer Inner Start --> ' +
                '<div class="cart-footer">' +
                '<ul class="price-content">' +
				'<%--<li> حمل و نقل <span> ۴۵۳۰۰۰۰ تومان </span></li>--%>' +
                '<li> مالیات <span id="sabadtax"> ' + mali +' تومان </span></li>' +
                '<li> مجموع <span id="sabadtotal"> ' + sumtp + ' تومان </span></li>' +
                '</ul>' +
                '<div class="cart-actions text-center">' +
                '<a class="btn btn-info cart-checkout" href="checkout.aspx">پرداخت</a>' +
                '</div>' +
                '</div>' +
                '<!-- Cart Footer Inner End -->';

            fin("sabad").innerHTML = "";
            fin("sabad").innerHTML = htll + footer;
            
        };
        function addtointerest(id) {
            NM.Shop.Admin.ShopAdmin.Set_Get_BI("Set", "interest", id, Set_Get_BI_SC000, WAError);
        };
        function Set_Get_BI_SC000(r) {
            NM.Shop.Admin.ShopAdmin.Set_Get_BI("get", "interest", 0, Set_Get_BI_SC001, WAError);
            alert(r);
        };
        function Set_Get_BI_SC0000(r) {
            NM.Shop.Admin.ShopAdmin.Set_Get_BI("get", "interest", 0, Set_Get_BI_SC001, WAError);
        };
        function Set_Get_BI_SC001(r) {
            var rty = r;
            var sumtp = 0;
            var htll = "";
            for (var i = 0; i < rty.length; i++) {
                htll += '<!-- Cart Box Start --> ' +
                    ' <div class="single-cart-box"> ' +
                    '<div class="cart-img"> ' +
                    '<a href="#"><img src="' + javalist(rty[i], "imageurl") + '" alt="cart-image"></a> ' +
                    //'<span class="pro-quantity">' + javalist(rty[i], "CNT") + '</span> ' +
                    '</div>' +
                    '<div class="cart-content"> ' +
                    '<h6><a href="product.aspx?PID=' + javalist(rty[i], "PId") + '">' + javalist(rty[i], "PName") + '</a></h6>' +
                    '<span class="cart-price">' + javalist(rty[i], "NPrice") + ' تومان</span>' +
                    '<span>سایز: S</span>' +
                    '<span>رنگ: زرد</span>' +
                    '</div>' +
                    '<a class="del-icone" href="javascript:deletfromint(' + javalist(rty[i], "PId") + ') "><i class="ion-close"></i></a>' +
                    '</div>' +
                    '<!-- Cart Box End -->';
            }
            fin("Alaka").innerHTML = htll;
        };
        function deletfromint(id) {
            NM.Shop.Admin.ShopAdmin.Set_Get_BI("delete", "interest", id, Set_Get_BI_SC0000, WAError);
        }

        function panelModiriat(x) {
            x.classList.toggle("StylePanelodirat").fadeIn(1000);
        }
      //  setInterval(function () {
        //    document.getElementsByClassName("nivo-nextNav")[0].click();
        //}, 3000);
    </script>
